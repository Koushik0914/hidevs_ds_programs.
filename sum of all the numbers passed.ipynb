   "source": [
    "def calculate_sum(*args):\n",
    "    add=0\n",
    "    for arg in args:\n",
    "        add=add+arg\n",
    "    return add\n",
    "\n",
    "print(calculate_sum(1, 2, 3, 4, 5))\n"

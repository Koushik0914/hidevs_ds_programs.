    "net_amount=0\n",
    "while True:\n",
    "    log=input(\"enter the D\\W amount or enter 'Done' to finish: \")\n",
    "    if log == \"Done\":\n",
    "        break\n",
    "    if log[0]=='D':\n",
    "        net_amount=net_amount+int(log[2:])\n",
    "    elif log[0]=='W':\n",
    "        net_amount=net_amount-int(log[2:])\n",
    "print(net_amount)\n",
    "        "
   ]
